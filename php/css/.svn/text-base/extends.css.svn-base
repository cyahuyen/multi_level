#services {
    padding: 10px;
}
#services a{
    color: white;
    text-decoration: underline;
}
#services h6{
    color: #F26C4F;
}
.label-text {
    font-weight: normal;
    display: block;
    color: white;
    text-align: left;
    margin-right: 1%;
    float: left;
    line-height: 24px;
    margin-top: 8px;
    width: 100%;
}
#info-user {
    border: 0px;
    width: 100%;
}
#info-user .label {
    color: white;
    text-align: right;
}
#info-user span.disabled { color: #cccccc; display: inline;font-size: 16px; }
#info-user span.enabled { color: #0099ff; display: inline; font-size: 16px;}
#info-user tr{
    border: 0px;
}
#info-user .headding {
    font-size: 16px;
    color: white;
}
.blance-left {
    float: left;
    clear: both;
    height: 15px;
    padding: 0px 10px;
}
.title-blance {
    float: left;
    padding-right: 15px;
    color: red;
    font-weight: bold;
}
.currency-blance {
    color: #FC0;
    font-size: 14px;
    font-weight: bold;
    float: left;
    display: block;
    text-align: left;
}
.log-user {
    float: left;
    clear: both;
    height: 15px;
}

.pagination {
    border-top: 1px solid #DDDDDD;
    display: inline-block;
    padding-top: 0px;
    width: 100%;
}
.pagination .links {
    float: left;
    padding-top: 6px;
}
.pagination .links a {
    border: 1px solid #DDDDDD;
    color: #A3A3A3;
    font-size: 10px;
    padding: 3px 5px;
    text-decoration: none;
}
.pagination .links b {
    background: none repeat scroll 0 0 #FFFFFF;
    border: 1px solid #269BC6;
    color: #269BC6;
    font-size: 10px;
    font-weight: normal;
    padding: 3px 6px;
    text-decoration: none;
}
.pagination .results {
    float: right;
    font-size: 10px;
    padding-top: 5px;
}
#logs {
    vertical-align: top;
}